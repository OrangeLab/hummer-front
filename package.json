    "@tweenjs/tween.js": "^17.6.0",
    "better-scroll": "^2.4.2",
    "hammerjs": "^2.0.8",
    "swiper": "7.0.8"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-strip": "^2.0.0",
    "@types/hammerjs": "^2.0.39",
    "@types/node": "^15.12.2",
    "@types/resize-observer-browser": "^0.1.6",
    "art-template": "^4.13.2",
